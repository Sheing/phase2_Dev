    <description>Provider Meeting - Updating Participation Status in Provider Meeting Participation</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(AND(ISCHANGED([Provider_Meeting__c].Meeting_Status__c ), ISPICKVAL([Provider_Meeting__c].Meeting_Status__c , &quot;Completed&quot;)),

OR(ISPICKVAL(PRIORVALUE([Provider_Meeting__c].Meeting_Status__c),&quot;Open for registration&quot;),
ISPICKVAL(PRIORVALUE([Provider_Meeting__c].Meeting_Status__c),&quot;Closed for registration&quot;)) 

) 
</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
