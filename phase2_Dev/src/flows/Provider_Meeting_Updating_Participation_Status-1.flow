OR(ISPICKVAL(PRIORVALUE({!myVariable_current.Meeting_Status__c}),&quot;Open for registration&quot;),
ISPICKVAL(PRIORVALUE({!myVariable_current.Meeting_Status__c}),&quot;Closed for registration&quot;)) 

)</expression>
    </formulas>
