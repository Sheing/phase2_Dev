              <h3 class="panel-title">
                Fiduciary / Administrative Relationships
              </h3>
            </div>
             <div class="panel-body">
             
             <div>             
             <apex:outputpanel id="fiduadmin" rendered="{!(LicenseRecordTypeName == 'Placement_New_License' || LicenseRecordTypeName == 'Placement_Move' ||LicenseRecordTypeName == 'Placement_Renewal' || LicenseRecordTypeName == 'Placement_Change_in_License')}">
                <apex:outputLabel styleClass="top-level-label" value="Does the applicant have fiduciary or administrative relationships to other child care agencies or independent contractors? " />                                    
                <apex:outputpanel >
                  <apex:selectradio value="{!licenseTransation.Existing_Fiduciary_Admin_relationships1__c}" disabled="true">
                    <apex:selectoptions value="{!items}" />
                  </apex:selectradio>
                </apex:outputpanel>
                </apex:outputpanel>
                </div>
                </div>
                </div>
                </apex:outputpanel>
                
                
                <div class="panel panel-default ">
            <div class="panel-heading">
              <h3 class="panel-title">
                Technical Assistance
              </h3>
            </div>
             <div class="panel-body">             
             <div>             
             <apex:outputpanel id="techasis">
                <apex:outputLabel styleClass="top-level-label" value="If you have concerns, questions, or would like information about regulations or policy issues, or other topics that affect your child care, please list below. This will assist you in preparing for your application process and enable your licensor to bring or send you resource materials, if available. " />                                    
                 <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Technical_Assistance__c}" />
                </apex:outputpanel>
                </div>
                </div>
                </div>
                
                
                 <div class="panel panel-default ">
            <div class="panel-heading">
              <h3 class="panel-title">
                Transaction Special Requests / Comments
              </h3>
            </div>
             <div class="panel-body">             
             <div>             
             <apex:outputpanel id="specialreq">
                <apex:outputLabel styleClass="top-level-label" value="If you have any special requests or comments associated with this transaction, please list below " />                                    
               <br/> <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Transaction_Special_Requests_Comments__c}" />
                </apex:outputpanel>
                </div>
                </div>
                </div>
                
                 <apex:outputpanel id="varaincereq" rendered="{!(LicenseRecordTypeName != 'R_P_Change_of_Admin_Authority' && licenseTransation.Variance_on_Regulations__r != null && licenseTransation.Variance_on_Regulations__r.size >0)}">
                <div class="panel panel-default ">
            <div class="panel-heading">
            <h3 class="panel-title">
                Variance Request
              </h3>
             </div> 
             <div class="panel-body">
              <div layout="row" layout-wrap="true">
                <apex:repeat value="{!licenseTransation.Variance_on_Regulations__r}" var="varian">
                    <div class="col-md-12 col-sm-12" style="padding: 0;">
                        <div class="slds-box slds-box--x-small " layout="column" layout-padding="true" layout-margin="true" style="overflow: hidden;">
                        <div layout="card-block row" layout-wrap="true">
                            <div class="card-row-item col-md-3 col-sm-12">
                                <p class="top-level-label">Regulation Number</p>
                                <p class="{!IF(varian.Associated_Regulation__r.Regulation_Number__c != null, '', 'empty-input')}">{!IF(varian.Associated_Regulation__r.Regulation_Number__c  != null, varian.Associated_Regulation__r.Regulation_Number__c , varian.Associated_Regulation__r.Regulation_Number__c )}</p>
                            </div>
                            
                            <div class="card-row-item col-md-3 col-sm-12">
                                <p class="top-level-label">Regulation Article</p>
                                <p class="{!IF(varian.Associated_Regulation__r.Article__c != null, '', 'empty-input')}">{!IF(varian.Associated_Regulation__r.Article__c  != null, varian.Associated_Regulation__r.Article__c , varian.Associated_Regulation__r.Article__c )}</p>
                            </div>                          
                            
                            <div class="card-row-item col-md-3 col-sm-12">
                                <p class="top-level-label">Describe your understanding of the regulation and the intent of the regulation</p>
                                <p class="{!IF(varian.Provider_Understanding_of_Regulation__c != null, '', 'empty-input')}">{!IF(varian.Provider_Understanding_of_Regulation__c  != null, varian.Provider_Understanding_of_Regulation__c , varian.Provider_Understanding_of_Regulation__c )}</p>
                            </div>
                            
                            <div class="card-row-item col-md-3 col-sm-12">
                                <p class="top-level-label">Describe your alternative method of compliance. Be specific, explaining how your method meets the regulation.</p>
                                <p class="{!IF(varian.Provider_Alternative_Method_to_Comply__c != null, '', 'empty-input')}">{!IF(varian.Provider_Alternative_Method_to_Comply__c  != null, varian.Provider_Alternative_Method_to_Comply__c , varian.Provider_Alternative_Method_to_Comply__c )}</p>
                            </div>
                 </div>
                 </div>
                 </div>
             </apex:repeat>
             </div>
             </div>
             </div>
            
             
             </apex:outputpanel>
             
                 
                
                <div class="panel panel-default"  style="display:{!if(loggedUser.UserType == 'PowerPartner' || loggedUser.UserType == 'PowerCustomerSuccess' || loggedUser.UserType == 'CspLitePortal' || loggedUser.UserType == 'CustomerSuccess', 'block','none')}">
            <div class="panel-heading">
              <h3 class="panel-title">
                 Disclaimer
              </h3>
            </div>
             <div class="panel-body">
             <apex:outputpanel id="disclaimer">
             <apex:outputText styleClass="top-level-label" value="I have read and understand the questions in this application. I have reviewed my answers to the application questions, and, to the best of my knowledge, the information I have provided and the responses I have given are true.

                            I understand that my application is valid for 90 days from the date it is received by EEC. If I do not complete the application process within 90 days, my file will be closed. If I still want to pursue my request, I will have to re-apply for and pay the applicable application fee again.

                            I understand that furnishing or making any misleading or false statements or reports anywhere in this application is grounds to revoke, suspend, refuse to issue or refuse to renew a family child care license.

                            I have read 606 CMR 7.00 Standards for the Licensure or Approval of Family Child Care; Small Group and School Age and Large Group and School Age Child Care Programs, and I agree to operate in compliance with the Department of Early Education and Care." />                
                </apex:outputpanel>
               </div>
               </div>
                
          
                  <div class="row">
                    <md-content class="navigation-buttons" layout="column" layout-lg="row" layout-align="space-between center" layout-gt-lg="row" layout-align-sm="center center" layout-align-md="center center">    
                        <apex:commandLink styleClass="md-button md-raised md-eec" value="Save and Previous"  action="{!saveandpreviousGSASummary}" html-layout-padding="true" html-flex-order-sm="1" html-flex-order-md="1" html-md-button="true"/>
                        <apex:commandLink styleClass="md-button md-raised md-eec" value="Save and Return to Provider" action="{!saveandreturnTOProviderGSASummary}"  html-layout-padding="true" html-flex-order-sm="1" html-flex-order-md="1" html-md-button="true"/>
                        
                    </md-content>
                    <md-content class="navigation-buttons" layout="column" layout-lg="row" layout-align="space-between center" layout-gt-lg="row" layout-align-sm="center center" layout-align-md="center center">    
                        <apex:commandLink styleClass="md-button md-raised"   immediate="true" action="{!CancelTransaction}" value="Cancel {!formattedLicenseRecordTypeLabel} Transaction"  onclick="return confirm('Are you sure to cancel the transaction?');" html-layout-padding="true" html-flex-order-sm="1" html-flex-order-md="1" html-md-button="true"/>
                        <apex:commandLink styleClass="md-button md-raised" value="Submit" rerender="showmsg,ownerShipAdminForm,SubmitPanel" action="{!submitRS}" html-layout-padding="true" html-flex-order-sm="1" html-flex-order-md="1" html-md-button="true" oncomplete="OnSubmit();"/>
                    </md-content>  
                </div>
                <apex:outputpanel id="SubmitPanel">
                  <script>
                      var status = '{!licenseTransation.Status__c}';
                      function OnSubmit(){
                      
                        if(status == 'Submitted' || status == 'Under Review'){
                            alert('Transaction has been submitted successfully.');
                            SubmitCompleted();
                        }
                      }
                  </script>
                </apex:outputpanel>

                <apex:actionFunction action="{!returnToProvider}" name="SubmitCompleted">
                </apex:actionFunction>
                    
                              <apex:actionFunction action="{!saveandpreviousGSASummary}" name="SavePrevious" oncomplete="addMargin();">
                       <apex:param name="previousPage" assignTo="{!previousPageName}" value=""/>
            </apex:actionFunction>            
                          
                            </apex:form> 
        </div>
        <eec-wizard class="col-lg-3" btn-progress-color="#fff" passed-btn-bg-color="#4CAF50" btn-bg-color="#FAFAFA" active-btn-bg-color="#0288D1" flex="20" hide="true" show-gt-md="true">
        	<c:EEC_Navigation current="Review & Submission"
                                                 recordTypeDeveloperName="{!LicenseRecordTypeName}"
                                                 providerRecordType="{!providerRecordType}"> 
         	</c:EEC_Navigation>
      	</eec-wizard>
        </div>
           <script>
    function addMargin(){
    	j$(".required-input").addClass("required-input-with-error");
    }
    function doNavigate(previousPage, nextPage){
	    if(nextPage != ''){
	    	SaveNext();
	    }
	    else if(previousPage != ''){
	    	SavePrevious(previousPage);
	    }
    }
    </script>
                            </body>
                            </apex:page>