                flex: 1 1 auto;
                -webkit-order: 2;
                -ms-flex-order: 2;
                order: 2;
                display: block;
                background: 0 0;
                padding: 2px 2px 1px;
                border-width: 0 0 1px;
                line-height: 26px;
                -ms-flex-preferred-size: 26px;
                border-radius: 0;
            }

            .requiredInput2 {
