             <div>
             <apex:outputpanel id="OutdoorSquare" >
                <apex:outputLabel styleClass="top-level-label" value="Outdoor Square Footage Used For The Operations :" />                                    
               <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Outdoor_Square_footage__c}" />
              </apex:outputpanel>
             <div>
                <apex:outputpanel id="Poolavailable" >
                 <apex:outputLabel styleClass="top-level-label" value="Do you have a pool that will be used during program hours? :" />  
             
                        
            <apex:selectradio value="{!licenseTransation.Pool_available_during_program_hours1__c}" disabled="true">
                            <apex:selectoptions value="{!pickitems}" />
                    </apex:selectradio>
                </apex:outputpanel>
               </div>
              <div>
                <apex:outputpanel id="Poolroof" >
                <apex:outputLabel styleClass="top-level-label" value="Latest Pool roof inspection date:" />                                    
                <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Pool_roof_inspection_date__c}" />
                </apex:outputpanel>
               </div>
              <div>
                <apex:outputpanel id="offsiteoutdoor" >
                <apex:outputLabel styleClass="top-level-label" value="Are you using an offsite outdoor play location?:" />                                    
               
                 <apex:selectradio value="{!licenseTransation.Offsite_outdoor_play_location1__c}" disabled="true">
                            <apex:selectoptions value="{!pickitems}" />
                    </apex:selectradio>
                </apex:outputpanel>
               </div>
              <div>
                <apex:outputpanel id="Commuteto" >
                <apex:outputLabel styleClass="top-level-label" value="How do you go there?:" />                                   
                <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Commute_to_offsite_play_location__c}" />
                </apex:outputpanel>
               </div>
               <div>
                <apex:outputpanel id="providetransportation" rendered="{!(licenseTransation.Available_Days_Full_day_program__c != NULL)}">
                <apex:outputLabel styleClass="top-level-label" value="Do you provide transportation?:" />                                    
                
                <apex:selectradio value="{!licenseTransation.Transportation_provided1__c}" disabled="true">
                            <apex:selectoptions value="{!pickitems}" />
                    </apex:selectradio>
                </apex:outputpanel>
               </div>
              <div>
                <apex:outputpanel id="ApprovedAdditional">
                <apex:outputLabel styleClass="top-level-label" value="Are you requesting the approval of any other additional space?:" />                                    
                <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Other_Approved_Additional_Space__c}" />
                </apex:outputpanel>
               </div>
            </div>
           </div>
         </div>
        </apex:outputpanel> 
        
         <apex:outputpanel id="compliance" rendered="{!(LicenseRecordTypeName !='GSA_Change_of_Admin_Authority' && LicenseRecordTypeName !='Close_Prior_Expiration' && LicenseRecordTypeName !='Change_of_Provider_Name' && LicenseRecordTypeName !='Re_issuance_of_License_Cert' )}"> 
         <div class="panel panel-default ">
         <div class="panel-heading">
              <h3 class="panel-title">
                Compliance with Americans with Disabilities Act (ADA) requirements
              </h3>
            </div>
             <div class="panel-body">
             <div>
             <apex:outputpanel id="IndoorADA" >
                <apex:outputLabel styleClass="top-level-label" value="Indoor :" />                                    
               <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Indoor_ADA_Compliant__c}" />
             </apex:outputpanel>
                  </div>
               <div>
                <apex:outputpanel id="OutdoorADA" >
                <apex:outputLabel styleClass="top-level-label" value="Outdoor / Playground:" />                                    
                <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Outdoor_ADA_Compliant__c}" />
                </apex:outputpanel>
               </div>
               </div>
               </div>
               </apex:outputpanel>
               <apex:outputpanel id="complddiance" rendered="{!(LicenseRecordTypeName !='Change_of_Provider_Name' && LicenseRecordTypeName !='Re_issuance_of_License_Cert')}"> 
               <div class="panel panel-default">
            <div class="panel-heading">
              <h3 class="panel-title">
                Technical Assistance
              </h3>
            </div>
             <div class="panel-body">
             <div>
             <apex:outputpanel id="TechnicalAssistance">
                <apex:outputLabel styleClass="top-level-label" value="If you have concerns, questions, or would like information about regulations or policy issues, or other topics that affect your child care, please list below. This will assist you in preparing for your application process and enable your licensor to bring or send you resource materials, if available:" />                                    
               <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Technical_Assistance__c}" />
               </apex:outputpanel>
             </div>
          </div>
      </div>
       </apex:outputpanel>
       <apex:outputpanel id="compddiance" rendered="{!(LicenseRecordTypeName !='Change_of_Provider_Name' && LicenseRecordTypeName !='Re_issuance_of_License_Cert')}">
        <div class="panel panel-default">
        <div class="panel-heading">
              <h3 class="panel-title">
                Transaction Special Requests / Comments
              </h3>
            </div>
             <div class="panel-body">
             <div>
             <apex:outputpanel id="Technicalrequest">
                <apex:outputLabel styleClass="top-level-label" value="If you have any special requests or comments associated with this transaction, please list below:" />                                    
               <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Transaction_Special_Requests_Comments__c}" />
              </apex:outputpanel>
               </div>
             </div>
         </div>
          </apex:outputpanel>
          <apex:outputpanel id="varasincereq" rendered="{!(LicenseRecordTypeName !='GSA_Change_of_Admin_Authority' && LicenseRecordTypeName !='Close_Prior_Expiration' && LicenseRecordTypeName !='Change_of_Provider_Name' && LicenseRecordTypeName !='Re_issuance_of_License_Cert' )}">
                <div class="panel panel-default ">
            <div class="panel-heading">
            <h3 class="panel-title">
                Variance Request
              </h3>
             </div> 
             <div class="panel-body">
              <div layout="row" layout-wrap="true">
                <apex:repeat value="{!licenseTransation.Variance_on_Regulations__r}" var="varian">
                    <div class="col-md-12 col-sm-12" style="padding: 0;">
                        <div class="slds-box slds-box--x-small " layout="column" layout-padding="true" layout-margin="true" style="overflow: hidden;">
                        <div layout="card-block row" layout-wrap="true">
                            <div class="card-row-item col-md-3 col-sm-12">
                                <p class="top-level-label">Regulation Number</p>
                                <p class="{!IF(varian.Associated_Regulation__r.Regulation_Number__c != null, '', 'empty-input')}">{!IF(varian.Associated_Regulation__r.Regulation_Number__c  != null, varian.Associated_Regulation__r.Regulation_Number__c , varian.Associated_Regulation__r.Regulation_Number__c )}</p>
                            </div>
                            
                            <div class="card-row-item col-md-3 col-sm-12">
                                <p class="top-level-label">Regulation Article</p>
                                <p class="{!IF(varian.Associated_Regulation__r.Article__c != null, '', 'empty-input')}">{!IF(varian.Associated_Regulation__r.Article__c  != null, varian.Associated_Regulation__r.Article__c , varian.Associated_Regulation__r.Article__c )}</p>
                            </div>                          
                            
                            <div class="card-row-item col-md-3 col-sm-12">
                                <p class="top-level-label">Describe your understanding of the regulation and the intent of the regulation</p>
                                <p class="{!IF(varian.Provider_Understanding_of_Regulation__c != null, '', 'empty-input')}">{!IF(varian.Provider_Understanding_of_Regulation__c  != null, varian.Provider_Understanding_of_Regulation__c , varian.Provider_Understanding_of_Regulation__c )}</p>
                            </div>
                            
                            <div class="card-row-item col-md-3 col-sm-12">
                                <p class="top-level-label">Describe your alternative method of compliance. Be specific, explaining how your method meets the regulation.</p>
                                <p class="{!IF(varian.Provider_Alternative_Method_to_Comply__c != null, '', 'empty-input')}">{!IF(varian.Provider_Alternative_Method_to_Comply__c  != null, varian.Provider_Alternative_Method_to_Comply__c , varian.Provider_Alternative_Method_to_Comply__c )}</p>
                            </div>
                 </div>
                 </div>
                 </div>
             </apex:repeat>
             </div>
             </div>
             </div>
            </apex:outputpanel>
            
            
            <md-content class="navigation-buttons" layout="column" layout-lg="row" layout-align="space-between center" layout-gt-lg="row" layout-align-sm="center center" layout-align-md="center center">
            <apex:commandlink styleclass="md-button md-raised md-eec" value="Back" action="{!backInstruction}" html-layout-padding="true" html-flex-order-sm="1" html-flex-order-md="1" html-md-button="true" />
            <apex:commandlink styleclass="md-button md-raised md-eec" value="Edit" action="{!editInstruction}" html-layout-padding="true" html-flex-order-sm="1" html-flex-order-md="1" html-md-button="true" rendered="{!licenseTransation.Status__c=='To Submit'}" />
            <apex:commandlink styleclass="md-button md-raised md-eec" value="View Associated Attachments" action="{!viewInstruction}" html-layout-padding="true" html-flex-order-sm="1" html-flex-order-md="1" html-md-button="true" rendered="{!Contains(licenseTransation.Status__c,'Closed')}" />
            <apex:commandlink styleclass="md-button md-raised md-eec" value="View / Update Associated Attachments" action="{!viewInstruction}" html-layout-padding="true" html-flex-order-sm="1" html-flex-order-md="1" html-md-button="true" rendered="{!!Contains(licenseTransation.Status__c,'Closed') && !Contains(licenseTransation.Status__c,'Cancelled') && licenseTransation.Status__c !='To Submit'}" />
          </md-content>
          <md-content class="navigation-buttons" layout="column" layout-lg="row" layout-align="space-between center" layout-gt-lg="row" layout-align-sm="center center" layout-align-md="center center">
            <apex:commandlink styleclass="md-button md-raised" action="{!CancelTransaction}" value="Cancel Transaction" immediate="true" onclick="return confirm('Are you sure to cancel the transaction?');" html-layout-padding="true" html-flex-order-sm="1" html-flex-order-md="1" html-md-button="true" rendered="{!licenseTransation.Status__c=='To Submit'}" />
          </md-content>
                   </apex:form> 
       </div>          
              </body>
    <script>
      app.controller('TransactionController', ['$scope', '$location', 'Util', function($scope, $location, Util) {

        // Instance of controller.
        var tranCtrl = this;
        // Controller level variables.
        tranCtrl.funcId = null;
        tranCtrl.wizardElements = Util.generateWizardElements("{!licenseRecordTypeName}", "Review & Submission");
        tranCtrl.selected = [];
      }]);
    </script>

</apex:page>