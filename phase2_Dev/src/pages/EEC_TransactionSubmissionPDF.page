<apex:page showHeader="false" standardStylesheets="false" sidebar="false" applyHtmlTag="false" applyBodyTag="false" renderAs="pdf" standardController="Licensing_Transaction__c" extensions="EEC_TransactionSubmissionPDFCtrl">
<style type="text/css">
table {
        -fs-table-paginate: paginate !important;
      }
.table-header {
        height: 30px !important;
      }
      
        .form-label{
          font-weight: bold;
          font-size:14px;
        }
        
 </style>
 <style>
table, th, td {
    
    border-collapse: collapse;
}
