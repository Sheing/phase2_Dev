                <div id="no-more-tables">
                  <table class="table table-bordered table-condensed cf {!IF(LicenseRecordTypeName =='FCC_Renewal','','not-visible')}">
                    <thead class="cf">
                      <tr>
                        <th class="panel-title">Topic</th>
                        <th class="panel-title">Date</th>
                        <th class="panel-title">Number of Hours</th>
                      </tr>
                    </thead>
                    <apex:repeat value="{!teList}" var="at">
                      <tr>
                        <td data-title="Additional Trainings"><apex:outputfield value="{!at.Name}" /></td>
                        <td data-title="Date">
                          <apex:outputtext value="{0,date,MM'/'dd'/'yyyy}">
                            <apex:param value="{!at.Date_of_the_Training__c}" />
                          </apex:outputtext>
                        </td>
                        <td data-title="Number of Hours"><apex:outputfield value="{!at.Number_of_Training_Hours__c}" /></td>
                      </tr>
                    </apex:repeat>
                  </table>
                </div>
              </apex:outputpanel>
            </div>
          </div>
          <apex:outputpanel rendered="{!IF(LicenseRecordTypeName == 'FCC_New_License'
                                            || LicenseRecordTypeName == 'FCC_Renewal'
                                            || LicenseRecordTypeName == 'FCC_License_Upgrade'
                                            || LicenseRecordTypeName == 'Change_of_Address'
                                            || LicenseRecordTypeName == 'FCC_Change_of_Household' ,'true','false')}">
            <div class="panel panel-default">
              <div class="panel-heading">
                <h3 class="panel-title">
                  Indoor Space Description
                </h3>
              </div>
              <div class="panel-body">
                <apex:outputpanel rendered="{!LicenseRecordTypeName =='FCC_New_License' || LicenseRecordTypeName=='Change_of_Address' || LicenseRecordTypeName=='FCC_Change_Room_Floor_Level'}">
                  <apex:outputlabel styleclass="top-level-label" value="Please describe the family child care residence: (e.g. single family, duplex, apartment, etc): " />
                  <apex:outputfield value="{!licenseTransation.FCC_Residence_Description__c}" />
                </apex:outputpanel>
                <br /><br />
                <div id="no-more-tables">
                  <table class="table table-condensed cf {!IF(LicenseRecordTypeName != 'FCC_Change_of_Household' && LicenseRecordTypeName != 'FCC_Downgrade_Capacity','','not-visible')}">
                    <thead class="cf">
                      <tr>
                        <th class="panel-title">Room Name</th>
                        <th class="panel-title">Use</th>
                        <th class="panel-title">Dimension - Length (feet)</th>
                        <th class="panel-title">Dimension - Width (feet)</th>
                        <th class="panel-title">Floor Level</th>
                      </tr>
                    </thead>
                    <apex:repeat value="{!roomList}" var="rm">
                      <tr>
                        <td data-title="Room Name"><apex:outputfield value="{!rm.Name}" /></td>
                        <td data-title="Use"><apex:outputfield value="{!rm.Use__c}" /></td>
                        <td data-title="Dimension - Length (feet)"><apex:outputfield value="{!rm.Dimension_Length_feet__c}" /></td>
                        <td data-title="Dimension - Width (feet)"><apex:outputfield value="{!rm.Dimension_Width_feet__c}" /></td>
                        <td data-title="Floor Level"><apex:outputfield value="{!rm.Floor_Level__c}" /></td>
                      </tr>
                    </apex:repeat>
                  </table>
                </div>
              </div>
            </div>

            <div class="panel panel-default {!IF(LicenseRecordTypeName=='FCC_Downgrade_Capacity' || LicenseRecordTypeName=='Variance_On_Regulation_Request','not-visible','')}">
              <div class="panel-heading">
                <h3 class="panel-title">
                  Additional Indoor Space Information
                </h3>
              </div>
              <div class="panel-body">
                <div class="ques">
                  <apex:outputlabel styleclass="top-level-label" value="Are there at least two exits from your home? " />
                  <apex:selectradio value="{!licenseTransation.At_Least_2_Exits__c}" disabled="true">
                    <apex:selectoptions value="{!items}" />
                  </apex:selectradio>
                </div>
                <div class="ques">
                  <apex:outputlabel styleclass="top-level-label" value="Please describe your exits. If you are using a basement space, there must be two exits from the basement level of the home: " />
                  <apex:outputfield style="margin-left:10px;" value="{!licenseTransation.Exits_Descriptions__c}" />
                </div>
                <div class="ques">
                  <apex:outputlabel styleclass="top-level-label" value="What types of heating elements do you have? (Check all that apply):" />
                  <div id="check-box-tables">
                    <apex:selectcheckboxes value="{!selectedHestElems}" styleclass="table" disabled="true">
                      <apex:selectoptions value="{!heatingElements}" />
                    </apex:selectcheckboxes>
                  </div>
                </div>
                <div id="nabhDesc" class="desc-answer">
                  <apex:outputpanel rendered="{!IF(((selectedHestStr =='Baseboards,') || (selectedHestStr == 'Vents / Grates,') || (selectedHestStr == 'Baseboards,Vents / Grates,')),false,true)}">
                    <apex:outputlabel styleclass="top-level-label" value="If you have anything other than baseboard or vents/grates, EEC Regulations require that the heating unit be barricaded in a way that keeps children safe. Do you need technical assistance on how to appropriately barricade heating units?: " />
                    <apex:selectradio value="{!licenseTransation.Need_Assist_to_Barricade_Heating_Units__c}" disabled="true">
                      <apex:selectoptions value="{!items}" />
                    </apex:selectradio>
                  </apex:outputpanel>
                </div>
                <div class="ques">
                  <apex:outputlabel styleclass="top-level-label" value="Is the drinking water in your home from: " />
                  <apex:outputfield value="{!licenseTransation.Drinking_Water_Source__c}" />
                </div>
              </div>
            </div>
          </apex:outputpanel>
          <div class="panel panel-default {!IF(LicenseRecordTypeName=='FCC_License_Upgrade','','not-visible')}">
            <div class="panel-heading">
              <h3 class="panel-title">
                Outdoor Space Description
              </h3>
            </div>
            <div class="panel-body">
              <apex:outputpanel >
                <apex:outputlabel styleclass="top-level-label" value="Have there been any changes since your last application in what you are using for outdoor play space?: " />
                <apex:selectradio value="{!licenseTransation.Outdoor_Space_Changed__c}" disabled="true">
                  <apex:selectoptions value="{!items}" />
                </apex:selectradio>
              </apex:outputpanel>
              <apex:outputpanel id="oscDesc" styleclass="desc-answer">
                &nbsp;
                <apex:outputpanel rendered="{!licenseTransation.Outdoor_Space_Changed__c == 'Yes'}">
                  <apex:outputtext styleclass="top-level-label" value="Please explain:" style="font-weight: bold !important;" />
                  <apex:outputfield value="{!licenseTransation.Outdoor_Space_Changes_Description__c}" />
                </apex:outputpanel>
              </apex:outputpanel>
            </div>
          </div>
          <div class="panel panel-default {!IF(LicenseRecordTypeName == 'FCC_New_License','','not-visible')}">
            <div class="panel-heading">
              <h3 class="panel-title">
                Daily Routine and Activities
              </h3>
            </div>
            <div class="panel-body">
              <apex:outputpanel >
                <apex:outputfield value="{!licenseTransation.Daily_Routine_and_Activities__c}" />
              </apex:outputpanel>
            </div>
          </div>
          <div class="panel panel-default {!IF(LicenseRecordTypeName == 'FCC_New_License' ||
                                LicenseRecordTypeName=='FCC_License_Upgrade' ||
                                LicenseRecordTypeName == 'FCC_Renewal' ||
                                LicenseRecordTypeName == 'Change_of_Address' ||
                                LicenseRecordTypeName == 'FCC_Change_Room_Floor_Level' ||
                                LicenseRecordTypeName == 'FCC_Change_of_Household','','not-visible')}">
            <div class="panel-heading">
              <h3 class="panel-title">
                Requested Technical Assistance
              </h3>
            </div>
            <div class="panel-body">
              <apex:outputpanel >
                <apex:outputfield value="{!licenseTransation.Technical_Assistance__c}" />
              </apex:outputpanel>
            </div>          
          </div>
          
           <div class="panel panel-default {!IF(varianceList.size > 0 && (LicenseRecordTypeName=='FCC_New_License' || LicenseRecordTypeName=='FCC_Renewal'|| LicenseRecordTypeName=='FCC_License_Upgrade'|| LicenseRecordTypeName=='Change_of_Address'|| LicenseRecordTypeName=='FCC_Change_Room_Floor_Level' || LicenseRecordTypeName == 'Variance_On_Regulation_Request'),'','not-visible')}">
            <div class="panel-heading">
              <h3 class="panel-title">
                Variance on Regulation Request
              </h3>
            </div>
            <div class="panel-body">
                <div id="no-more-tables">
                  <table class="table table-condensed cf {!IF((LicenseRecordTypeName=='FCC_New_License' || LicenseRecordTypeName=='FCC_Renewal' || LicenseRecordTypeName=='FCC_License_Upgrade'|| LicenseRecordTypeName=='Change_of_Address'|| LicenseRecordTypeName=='FCC_Change_Room_Floor_Level' || LicenseRecordTypeName == 'Variance_On_Regulation_Request'),'','not-visible')}">
                    <thead class="cf">
                      <tr>
                        <th class="panel-title">Regulation Number</th>
                        <th class="panel-title">Regulation Article</th>
                        <th class="panel-title">Your understanding of the regulation and the intent of the regulation</th>
                        <th class="panel-title">Your alternative method of compliance</th>
                      </tr>
                    </thead>
                    <apex:repeat value="{!varianceList}" var="vl">
                      <tr>
                        <td data-title="Regulation" width="10%"><apex:outputfield value="{!vl.Associated_Regulation__r.Regulation_Number__c}" /></td>
                        <td data-title="Regulation Article" width="50%"><apex:outputfield value="{!vl.Associated_Regulation__r.Article__c}" /></td>
                        <td data-title="Understanding of the regulation" width="20%"><apex:outputfield value="{!vl.Provider_Understanding_of_Regulation__c}" /></td>
                        <td data-title="Alternative method of compliance" width="20%"><apex:outputfield value="{!vl.Provider_Alternative_Method_to_Comply__c}" /></td>
                      </tr>
                    </apex:repeat>
                  </table>
                </div>
            </div>
          </div>

          <div class="panel panel-default {!IF(LicenseRecordTypeName == 'FCC_Change_of_Name' || LicenseRecordTypeName == 'FCC_Provider_Assistant_Update' || LicenseRecordTypeName == 'FCC_Downgrade_Capacity','not-visible', '')}">
            <div class="panel-heading">
              <h3 class="panel-title">
                Transaction Special Requests / Comments
              </h3>
            </div>
            <div class="panel-body">
              <apex:outputpanel >               
                <apex:outputlabel styleclass="top-level-label" value="Special Requests and Comments:" />
                <apex:outputfield value="{!licenseTransation.Transaction_Special_Requests_Comments__c}" />
              </apex:outputpanel>
            </div>
          </div>

          <div class="panel panel-default {!IF(LicenseRecordTypeName == 'FCC_Change_of_Name' || LicenseRecordTypeName == 'FCC_Provider_Assistant_Update' || LicenseRecordTypeName == 'FCC_Downgrade_Capacity','not-visible','')}">
            <div class="panel-heading">
              <h3 class="panel-title">Attachments</h3>
            </div>
            <div class="panel-body">
              <apex:outputpanel id="fileAttachmentContainer" styleclass="fileAttachmentContainer" style="width: 100%;">
                <apex:outputpanel id="fileListContainer">
                  <!-- Incident Attachments -->
                  <apex:outputpanel rendered="{!attachments.size > 0}">
                    <apex:repeat value="{!attachments}" var="attachment">
                      <div class="attachment-item">
                        <p>{!attachment.Filename__c}</p>
                        <p><apex:outputtext value="{0, date, MMMM d','  yyyy}"><apex:param value="{!attachment.CreatedDate}" /></apex:outputtext></p>
                      </div>
                    </apex:repeat>
                  </apex:outputpanel>
                  <!-- No Attachments -->
                  <apex:outputpanel rendered="{!attachments.size == 0}">
                    <p>No attachments for this Licensing Transaction.</p>
                  </apex:outputpanel>
                </apex:outputpanel>
              </apex:outputpanel>
            </div>
          </div>

          <md-content class="navigation-buttons" layout="column" layout-lg="row" layout-align="space-between center" layout-gt-lg="row" layout-align-sm="center center" layout-align-md="center center">
            <apex:commandlink styleclass="md-button md-raised md-eec" value="Back" action="{!backInstruction}" html-layout-padding="true" html-flex-order-sm="1" html-flex-order-md="1" html-md-button="true" />
            <apex:commandlink styleclass="md-button md-raised md-eec" value="Edit" action="{!editInstruction}" html-layout-padding="true" html-flex-order-sm="1" html-flex-order-md="1" html-md-button="true" rendered="{!licenseTransation.Status__c=='To Submit'}" />
          </md-content>
          <md-content class="navigation-buttons" layout="column" layout-lg="row" layout-align="space-between center" layout-gt-lg="row" layout-align-sm="center center" layout-align-md="center center">
            <apex:commandlink styleclass="md-button md-raised"   immediate="true" action="{!CancelTransaction}" value="Cancel Transaction" onclick="return confirm('Are you sure to cancel the transaction?');" html-layout-padding="true" html-flex-order-sm="1" html-flex-order-md="1" html-md-button="true" rendered="{!licenseTransation.Status__c=='To Submit'}" />
          </md-content>
        </apex:form>
      </div>
    </div>
    <c:EEC_Site_Footer id="siteFooter" />
    <script>
      app.controller('TransactionController', ['$scope', '$location', 'Util', function($scope, $location, Util) {

        // Instance of controller.
        var tranCtrl = this;
        // Controller level variables.
        tranCtrl.funcId = null;
        tranCtrl.wizardElements = Util.generateWizardElements("{!licenseRecordTypeName}", "Review & Submission");
        tranCtrl.selected = [];
      }]);
    </script>
  </body>  
</apex:page>