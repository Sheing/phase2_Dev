<apex:page showheader="false" standardstylesheets="false" sidebar="false" controller="EEC_PortalMyProviderPageController">
  <head>
    <title>EEC : Portal Provider Page</title>
    <c:EEC_Header id="pageHeader" />
    <script>
        j$(document).ready(function(){
            j$("[id*=fccState]").prop("disabled",false);
            j$('form').bind('submit', function () {
                j$(this).find(':disabled').removeAttr('disabled');
            });
        });
        function changeState(state){
            var regExp = /\(([^)]+)\)/;
            var st = regExp.exec(j$("select option[value = '" + state.value + "']").text());
            j$("[id*=fccState]").val(st);
        }
    </script>
    <style type="text/css">
      .form-label {
        font-weight: bold;
        font-size: 14px;
      }

      thead {
        background-color: #f5f5f5;
      }

      .header-button {
        margin-right: 3px;
      }

      .not-visible {
        display: none;
      }

      .form-group {
        width: 100%;
      }

      .btn-group {
        margin-bottom: 100px;
      }
       .top-level-label {
                font-weight: 600;
                margin-right: 5px;
                line-height: 20px; 
                color: rgba(0,0,0,0.54);
              }

      .table > thead:first-child > tr:first-child > th {
        border-bottom: 0px;
        border-top: 1px solid #dddddd;
        font-weight: bold;
        font-size: 14px;
      }

      .table {
        border-left: 1px solid #dddddd;
        border-right: 1px solid #dddddd;
        border-bottom: 1px solid #dddddd;
      }
       .radio-group .parsley-errors-list {
            margin-top: 20px;
            margin-left: -50px;
            position: absolute;
            width:50%;
       }
       .radio-group .parsley-error {
            border: 0px; 
       }
        .radio-group label{
        font-weight : normal;  
      }

      .licensing-table > thead:first-child > tr:first-child > th {
        width: 135.43px;
      }

      .table-group-one .table > thead:first-child > tr:first-child > th {
        width: 135.43px;
      }

      .table-group-two .table > thead:first-child > tr:first-child > th {
        width: 189.6px;
      }

      .provider-container {
        margin-bottom: 100px;
      }

      @media only screen and (max-width: 800px) {
        .right-group {
          padding-right: 3px;
          width: 100%;
        }
      }

      @media only screen and (max-width: 959px) and (min-width: 0px) {
        .navigation-buttons input, a {
          font-size: 12px;
        }
      }

      md-card {
        font-size: 14px;
      }
    </style>
  </head>
  <body>
    <c:EEC_Nav_Bar id="navBar" />
    <div class="container">
      <apex:form >
        <div class="provider-container" layout="row" layout-margin="true" layout-padding="true" layout-align="center center" flex="100">
          <div class="provider-information" layout="column" flex="90" flex-sm="90">
            <apex:outputpanel rendered="{!errorMsg != null}">
              <h2>{!errorMsg}</h2>
            </apex:outputpanel>
            <apex:outputpanel id="userDetail">
              <br />
              <apex:outputpanel rendered="{!account.Recordtype.Name = 'FCC Assistant' && isEdit}">
                <h2 class="pull-left col-lg-4">Assistant</h2>
              </apex:outputpanel>
              <apex:outputpanel rendered="{!account.Recordtype.Name = 'FCC Assistant' && !isEdit}">
                <h2 class="pull-left col-lg-4">{!account.Recordtype.Name} Detail</h2>
              </apex:outputpanel>
              <apex:outputpanel rendered="{!account.Recordtype.Name != 'FCC Assistant'}">
                <h2 class="pull-left col-lg-4" style="width:100%">{!account.Recordtype.Name} Provider</h2>
              </apex:outputpanel>
              <br /><br /><br /><br />
              <div class="navigation-buttons" layout="column" layout-lg="row" layout-align="space-between center" layout-gt-lg="row" layout-align-sm="center center" layout-align-md="center center">
                <div class="right-group md-button">
                  <apex:commandbutton action="{!edit}" styleclass="md-button md-raised md-eec header-button" rendered="{!!isEdit && profileName != 'Community Standard User without transactions for EEC'}" value="{!$Label.site.edit}" id="editButton" />
                </div>
                <div class="right-group pull-right md-button {!IF((account.Recordtype.Name = 'Group Care' || account.Recordtype.Name = 'Temporary Shelter') && account.Use_Physical_Restraints__c = true && isEdit == false && account.Recordtype.Name != 'FCC Assistant', '', 'not-visible')}">
                  <apex:commandbutton html-layout-fill="true" styleclass="md-button md-raised md-eec" value="Submit New Quarterly Restraint Report" action="{!resReport}"></apex:commandbutton>
                </div>
                <div class="right-group pull-right md-button {!IF(!isEdit && account.Recordtype.Name != 'FCC Assistant', '' ,'not-visible')}">
                  <apex:commandbutton html-layout-fill="true" styleclass="md-button md-raised md-eec" value="Submit New Incident / Injury" action="{!subInc}"></apex:commandbutton>
                </div>
                <div class="right-group pull-right md-button {!IF(!isEdit, '' ,'not-visible')}">
                  <apex:commandbutton action="{!noncomp}" styleclass="md-button md-raised md-eec header-button" rendered="{!!isEdit && account.Recordtype.Name != 'FCC Assistant'}" value="Fill Non-Compliance Corrective Actions" id="noncompButton" />
                </div>
                <div class="right-group pull-right md-button {!IF(!isEdit, '' ,'not-visible')}">
                  <apex:outputpanel styleclass="right-group">
                    <apex:commandbutton action="{!trxIntro}" styleclass="md-button md-raised md-eec header-button" rendered="{!!isEdit && profileName != 'Community Standard User without transactions for EEC'}" value="New Licensing Transaction" id="licenseButton" />
                  </apex:outputpanel>
                </div>
              </div>
              <br />
              <apex:pageblock id="editUserDetail" rendered="{!isEdit}">
                <apex:pagemessages />
                <section class="contact-information">
                  <apex:outputpanel rendered="{!account.Recordtype.Name != 'FCC Assistant'}">
                    <h4 class="md-title">Provider Information</h4>
                  </apex:outputpanel>
                  <apex:outputpanel rendered="{!account.Recordtype.Name = 'FCC Assistant'}">
                    <h4 class="md-title">Assistant Information</h4>
                  </apex:outputpanel>
                  <md-divider></md-divider><br />
                  <div class="row">
                    <div class="col-lg-6">
                      <div class="md-subhead" layout="row">
                        <p flex-sm="80" flex="90"><strong>Name: </strong>{!account.name}</p>
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="md-subhead" layout="row">
                        <p flex-sm="80" flex="90"><strong>Type:</strong> {!account.Recordtype.Name}</p>
                      </div>
                    </div>
                  </div>
                </section>
                <section class="contact-information">
                  <h4 class="md-title">Contact Information</h4>
                  <md-divider></md-divider><br />
                  <div class="row">
                    <div class="col-lg-6">
                      <div class="md-subhead" layout="row">
                        <div class="form-group">
                          <label>Phone : </label>
                          <apex:inputfield styleclass="form-control" value="{!account.Phone}" />
                        </div>
                      </div>
                      <div class="md-subhead {!IF((account.Recordtype.Name = 'Family Child Care' || account.Recordtype.Name = 'FCC Assistant' || account.Recordtype.Name = 'Adoption Agency'|| account.Recordtype.Name = 'Foster Care Agency' || account.Recordtype.Name ='Group Care'|| account.Recordtype.Name = 'Large Group' || account.Recordtype.Name ='Small Group'|| account.Recordtype.Name ='Temporary Shelter'), '', 'not-visible')}" layout="row">
                        <div class="form-group">
                          <label>Alternate Phone : </label>
                          <apex:inputfield styleclass="form-control" value="{!account.Alternate_Phone__c}" />
                        </div>
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="md-subhead" layout="row"> 
                        <div class="form-group">
                          <label>Fax : </label>
                          <apex:inputfield styleclass="form-control" value="{!account.fax}" />
                        </div>
                      </div>
                      <div class="md-subhead" layout="row">
                        <div class="form-group">
                          <label>Email : </label>
                          <apex:inputfield styleclass="form-control" value="{!account.Provider_Email__c}" />
                        </div>
                      </div>
                    </div>
                  </div>
                </section>
                <section class="contact-information">
                  <h4 class="md-title">Main Address Information</h4>
                  <md-divider></md-divider><br />
                  <div class="row">
                    <div class="col-lg-6">
                      <div class="md-subhead" layout="row">
                        <div class="form-group">
                          <label>Street Address 1 :&nbsp;</label>
                          <apex:outputpanel rendered="{!account.Recordtype.Name = 'FCC Assistant'}">
                            <apex:inputfield styleclass="form-control" value="{!account.Provider_Address_1__c}" label="Contact Name" />
                          </apex:outputpanel>
                          <apex:outputpanel rendered="{!account.Recordtype.Name != 'FCC Assistant'}">
                            <apex:outputfield styleclass="form-control" value="{!account.Provider_Address_1__c}" label="Contact Name" />
                          </apex:outputpanel>
                        </div>
                      </div>
                      <div class="md-subhead" layout="row">
                        <div class="form-group">
                          <label>Street Address 2 :&nbsp;</label>
                          <apex:inputfield styleclass="form-control" rendered="{!account.Recordtype.Name = 'FCC Assistant'}" value="{!account.Provider_Address_2__c}" label="Contact Name" />
                          <apex:outputfield styleclass="form-control" rendered="{!account.Recordtype.Name != 'FCC Assistant'}" value="{!account.Provider_Address_2__c}" label="Contact Name" />
                        </div>
                      </div>
                      <div class="md-subhead" layout="row">
                        <div class="form-group">
                          <label>City :&nbsp;</label>
                          <apex:outputtext value="{!account.Provider_City__r.Name}" rendered="{!account.Recordtype.Name != 'FCC Assistant'}" label="Contact Name" />
                          <apex:selectlist onchange="changeState(this);" rendered="{!account.Recordtype.Name == 'FCC Assistant'}" styleclass="form-control" id="maCityPicklist" value="{!account.Provider_City__c}" size="1" html-data-parsley-required="true">
                            <apex:selectOptions value="{!maCityList.0}"/> 
                            <apex:selectOptions value="{!maCityList.1}"/> 
                            <apex:selectOptions value="{!maCityList.2}"/>
                          </apex:selectlist>  
                        </div>
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="md-subhead" layout="row">
                        <div class="form-group">
                          <label>State :&nbsp;</label>
                          <apex:inputfield id="fccState" styleclass="form-control"  rendered="{!account.Recordtype.Name = 'FCC Assistant'}" value="{!account.State__c}" />
                          <apex:outputfield styleclass="form-control" rendered="{!account.Recordtype.Name != 'FCC Assistant'}" value="{!account.State__c}" />
                        </div>
                      </div>
                      <div class="md-subhead" layout="row">
                        <div class="form-group">
                          <label>Zip Code :&nbsp;</label>
                          <apex:inputfield styleclass="form-control" rendered="{!account.Recordtype.Name = 'FCC Assistant'}" value="{!account.Provider_Zip_Code__c}" label="Contact Name" />
                          <apex:outputfield styleclass="form-control" rendered="{!account.Recordtype.Name != 'FCC Assistant'}" value="{!account.Provider_Zip_Code__c}" label="Contact Name" />
                        </div>
                      </div>
                    </div>
                  </div>
                </section>
                <section class="contact-information">
                  <h4 class="md-title">Mailing Address Information</h4>
                  <md-divider></md-divider><br />
                  <div class="row">
                    <div class="col-lg-6">
                      <div class="md-subhead" layout="row">
                        <div class="form-group">
                          <label>Street Address </label>
                          <apex:inputfield styleclass="form-control" value="{!account.ShippingStreet}" label="Contact Name" style="min-height:65px;" />
                        </div>
                      </div>
                      <div class="md-subhead" layout="row" style="min-height: 98px;">
                        <div class="form-group">
                          <label>City </label>
                          <apex:inputfield styleclass="form-control" value="{!account.ShippingCity}" label="Contact Name" />
                        </div>
                      </div>  
                    </div>
                    <div class="col-lg-6">
                      <div class="md-subhead" layout="row">
                        <div class="form-group">
                          <label>State </label>
                          <apex:inputfield styleclass="form-control" value="{!account.ShippingState}" label="Contact Name" />
                        </div>
                      </div>
                      <div class="md-subhead" layout="row">
                        <div class="form-group">
                          <label>Zip Code </label>
                          <apex:inputfield styleclass="form-control" value="{!account.ShippingPostalCode}" label="Contact Name" />
                        </div>
                      </div>  
                    </div>
                  </div> 
                </section>  
                               
                   
                <apex:outputpanel rendered="{!IF(account.Recordtype.Name = 'Family Child Care' || account.Recordtype.Name = 'FCC Assistant' , true,false)}">
                  <section class="program-information">
                    <h4 class="md-title">Program Information</h4>
                    <md-divider></md-divider><br />
                    <div class="row">
                      <div class="col-lg-6">
                        <div class="md-subhead" layout="row">
                          <div class="form-group">
                            <label>Language</label>
                            <apex:inputfield styleclass="form-control" value="{!account.Language__c}" label="Language" />
                          </div>
                        </div>
                        <div class="md-subhead" layout="row">
                          <div class="form-group">
                            <label>Preferred Communication Language</label>
                            <apex:inputfield styleclass="form-control" value="{!account.Preferred_Language__c}" label="Preferred Language" />
                          </div>
                        </div>
                      </div>
                      <div class="col-lg-6">
                        <div class="md-subhead" layout="row"> 
                          <div class="radio-group"  flex="70">
                            <label>Can you communicate in English?</label>
                          <!--  <apex:inputfield styleclass="form-control" value="{!account.Can_Communicate_in_English__c}" label="Communicate in English" /> -->
                             <apex:selectradio value="{!account.Can_Communicate_in_English__c}" styleclass="top-level-label" style="width:30%"> 
                                <apex:selectoptions value="{!items}" /> 
                                </apex:selectradio>  
                          </div>
                        </div> 
                        
                       
                        <div class="md-subhead" layout="row">
                          <div class="form-group">
                            <label>Ethnicity </label>
                            <apex:inputfield styleclass="form-control" value="{!account.Race__c}" label="Ethnicity" />
                          </div>
                        </div>
                      </div>
                    </div>  
                  </section>
                </apex:outputpanel>
                <div class="btn-group pull-right">
                  <md-content class="navigation-buttons" layout="column" layout-lg="row" layout-align="space-between center" layout-gt-lg="row" layout-align-sm="center center" layout-align-md="center center">
                    <apex:commandbutton id="save" value="{!$Label.site.save}" styleclass="md-button md-raised md-eec pull-right" action="{!save}" rendered="{!isEdit}" />
                    <apex:commandbutton id="cancel" value="{!$Label.site.cancel}" styleclass="md-button md-raised md-eec pull-right" action="{!cancel}" rendered="{!isEdit}" onclick="return confirm('Are sure to cancel the changes?');" />
                  </md-content>
                </div>
              </apex:pageblock>
              <apex:pageblock id="viewUserDetail" rendered="{!!isEdit}">
                <div class="row">
                  <div class="col-lg-12">
                    <section class="contact-information">
                      <apex:outputpanel rendered="{!account.Recordtype.Name != 'FCC Assistant'}">
                        <h4 class="md-title">Provider Information</h4>
                      </apex:outputpanel>
                      <apex:outputpanel rendered="{!account.Recordtype.Name = 'FCC Assistant'}">
                        <h4 class="md-title">Assistant Information</h4>
                      </apex:outputpanel>
                      <md-divider></md-divider><br />
                      <div class="row">
                        <div class="col-lg-6">
                          <div class="md-subhead" layout="row">
                            <p flex-sm="80" flex="90"><strong>Name: </strong>{!account.name}</p>
                          </div>
                          <apex:outputpanel rendered="{!IF(account.Recordtype.Name = 'Family Child Care' || account.Recordtype.Name = 'FCC Assistant', false,true)}">
                            <div class="md-subhead" layout="row">
                              <p flex-sm="80" flex="90"><strong>Umbrella:</strong> <a href="/EEC_Umbrella" target="_top">{!account.Umbrella_Members__r.Name}</a></p>
                            </div>
                          </apex:outputpanel>
                          <apex:outputpanel rendered="{!IF(account.Recordtype.Name != 'Family Child Care', false,true)}">
                            <div class="md-subhead" layout="row">
                              <p flex-sm="60" flex="70"><strong>FCC System:</strong> {!FCCSystemName}</p>
                              <p flex-sm="40" flex="30"><a href="mailto:{!user.Email}?Subject=Change%20of%20FCC%20System%20request%20from%20{!HTMLENCODE(account.Name)}">Request a change</a></p>
                            </div>
                          </apex:outputpanel>
                          <apex:outputpanel rendered="{!IF(account.Recordtype.Name = 'Family Child Care' || account.Recordtype.Name = 'FCC Assistant', false,true)}">
                            <div class="md-subhead" layout="row">
                              <p flex-sm="80" flex="90"><strong>Business Name:</strong>{!account.Business_Name__c}</p>
                            </div>
                          </apex:outputpanel>
                          <div class="md-subhead" layout="row">
                            <p flex-sm="80" flex="90"><strong>Type: </strong>{!account.Recordtype.Name}</p>
                          </div>
                          <div class="md-subhead" layout="row">
                            <p flex-sm="80" flex="90"><strong>Number:</strong> {!account.Provider_Number__c}</p>
                          </div>
                        </div>
                        <div class="col-lg-6">
                          <div class="md-subhead" layout="row">
                            <apex:outputpanel rendered="{!account.Recordtype.Name != 'FCC Assistant'}">  
                                <p flex-sm="80" flex="90"><strong>License Status:</strong> {!account.License_Status__c}</p>
                            </apex:outputpanel>
                            <apex:outputpanel rendered="{!account.Recordtype.Name == 'FCC Assistant'}">  
                                <p flex-sm="80" flex="90"><strong>Certificate Status:</strong> {!account.License_Status__c}</p>
                            </apex:outputpanel>  
                          </div>
                          <div class="md-subhead" layout="row">
                            <p flex-sm="80" flex="90"><strong>Provider Status:</strong> {!account.Provider_Status__c}</p>
                          </div>
                          <div class="md-subhead" layout="row">
                            <p flex-sm="80" flex="90"><strong>Tax ID:</strong> {!account.Tax_ID__c}</p>
                          </div>
                        </div>
                      </div>
                    </section>
                  </div>
                  <br />
                </div>
                <div class="row">
                  <div class="col-lg-12">
                    <section class="contact-information">
                      <h4 class="md-title">Your Licensor</h4>
                      <md-divider></md-divider><br />
                      <div class="row">
                        <div class="col-lg-12">
                          <div class="md-subhead" layout="row">
                            <p flex-sm="80" flex="90"><strong>First / Last Name: </strong>{!user.name}</p>
                          </div>
                          <div class="md-subhead" layout="row">
                            <p flex-sm="80" flex="90"><strong>Phone: </strong>{!user.phone}</p>
                          </div>
                          <div class="md-subhead" layout="row">
                            <p flex-sm="80" flex="90"><strong>Email: </strong>{!user.Email}</p>
                          </div>
                        </div>
                      </div>
                    </section>
                  </div>
                </div>
                <br />
                
                  
                 <div align="left"  class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
                  <div class="panel panel-default">
                    <div class="panel-heading collapsed" data-toggle="collapse" data-parent="#otherInfo" href="#otherInfo" aria-expanded="false" aria-controls="otherInfo" role="tab" id="headingThree">
                      <h4 class="panel-title">
                        <a>
                          <i class="fa fa-chevron-right"></i>&nbsp; Other Information
                        </a>
                      </h4>
                    </div>
                    <div id="otherInfo" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingThree">
                      <div class="panel-body">
                        <div class="col-lg-6">
                          <div  class="form-group {!IF(account.Recordtype.Name != 'FCC Assistant','','not-visible')}">
                            <div class="input-group" >
                              <span class="form-label"  style="text-align:left;">Head Start:  </span>
                              <apex:outputfield styleclass="form-control" value="{!account.Head_Start__c}" />
                            </div>
                          </div>   
                          <div class="form-group">
                                <div class="input-group {!IF(account.Recordtype.Name == 'FCC Assistant','','not-visible')}">
                                  <span class="form-label">Preferred Language: </span>
                                  <apex:outputfield styleclass="form-control" value="{!account.Preferred_Language__c}" rendered="{!IF(account.Recordtype.Name = 'Family Child Care' || account.Recordtype.Name = 'FCC Assistant', true,false)}" />
                                </div>
                              </div>  
                          <div class="form-group {!IF(account.Recordtype.Name == 'FCC Assistant','','not-visible')}">
                                <div class="input-group">
                                  <span class="form-label">Language: </span>
                                  <apex:outputfield styleclass="form-control" value="{!account.Language__c}" rendered="{!IF(account.Recordtype.Name = 'Family Child Care' || account.Recordtype.Name = 'FCC Assistant', true,false)}" />
                                </div>
                              </div>    
                        </div>  
                        <div class="col-lg-6">
                          
                          <div class="form-group {!IF(account.Recordtype.Name == 'FCC Assistant','','not-visible')}">
                                <div class="input-group">
                                  <span class="form-label">Ethnicity: </span>
                                  <apex:outputfield styleclass="form-control" value="{!account.Race__c}" rendered="{!IF(account.Recordtype.Name = 'Family Child Care' || account.Recordtype.Name = 'FCC Assistant', true,false)}" />
                                </div> 
                          </div> 
                          <div class="form-group {!IF(account.Recordtype.Name == 'FCC Assistant','','not-visible')}">
                                <div class="input-group">
                                  <span class="form-label">Bi-Lingual Licensor </span>
                                  <apex:outputfield styleclass="form-control" value="{!account.Bi_lingual_Licensor__c}" rendered="{!IF(account.Recordtype.Name = 'Family Child Care' || account.Recordtype.Name = 'FCC Assistant', true,false)}" />
                                </div>
                          </div>                          
                                               
                        </div>                        
                    </div>
                  </div>
                  </div>    
                  <div class="panel panel-default">
                    <div class="panel-heading collapsed" data-toggle="collapse" data-parent="#providerContact" href="#providerContact" aria-expanded="false" aria-controls="providerContact" role="tab" id="headingThree">
                      <h4 class="panel-title">
                        <a>
                          <i class="fa fa-chevron-right"></i>&nbsp;&nbsp;
                            <apex:outputText rendered="{!account.Recordtype.Name == 'FCC Assistant'}">Assistant </apex:outputText>
                            <apex:outputText rendered="{!account.Recordtype.Name != 'FCC Assistant'}">Provider </apex:outputText>
                            Contact Information
                        </a>
                      </h4>
                    </div>
                    <div id="providerContact" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingThree">
                      <div class="panel-body">
                     
                          <div class="col-lg-6">
                            <div class="form-group">
                              <div class="input-group">
                                <span class="form-label">Phone:  </span>
                                <apex:outputfield styleclass="form-control" value="{!account.Phone}" />
                              </div>
                            </div>
                            <div class="form-group {!IF((account.Recordtype.Name = 'Family Child Care' || account.Recordtype.Name = 'FCC Assistant' || account.Recordtype.Name = 'Small Group' ||  account.Recordtype.Name = 'Adoption Agency' || account.Recordtype.Name = 'Group Care' || account.Recordtype.Name = 'Foster Care Agency'|| account.Recordtype.Name = 'Temporary Shelter'  || account.Recordtype.Name = 'Large Group'), '', 'not-visible')}">
                              <div class="input-group">
                                <span class="form-label">Alternate Phone: </span>
                                <apex:outputfield styleclass="form-control" value="{!account.Alternate_Phone__c}" />
                              </div>
                            </div>
                          </div>
                          <div class="col-lg-6">
                            <div class="form-group">
                              <div class="input-group">
                                <span class="form-label">Fax: </span>
                                <apex:outputfield styleclass="form-control" value="{!account.fax}" />
                              </div>
                            </div>
