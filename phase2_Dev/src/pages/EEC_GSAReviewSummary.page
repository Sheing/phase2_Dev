<apex:page controller="EEC_TransactionWizardController" docType="HTML-5.0" showheader="false" sidebar="false" standardStylesheets="false" action="{!initGSA_RSubmissionPage}">
    <head>
        <title>EEC</title>
        <c:EEC_Header id="pageHeader" />
        <style>
        .bPageBlock .requiredInput .requiredBlock { 
        background-color: #c00;
        position: absolute;
        left: -4px;
        width: 3px;
        top: 1px;
        bottom: 1px;
        top: 20px;
        display: none;
      }
            .add-button-container{
                margin-bottom : 30px;
            }
            .background-form-container .errorMsg{ 
                width: 300px;
                padding-left: 50px;
            }
             .parsley-error {
        border: 2px solid #c00;
      }
       .my_error_container{
        position:absolute;
        bottom: 90px; 
        width:50%;
      }  
        
      .errorMsg {
        color: #d74c3b; 
      }
        
      .message[class*='error'] {
        background-color: #eee;
        padding: 0 20px 10px;
        margin-bottom: 25px;
        border-radius: 3px;
        -webkit-box-shadow: 0px 0px 5px 0px rgba(204,0,0,0.4);
        -moz-box-shadow: 0px 0px 5px 0px rgba(204,0,0,0.4);
        box-shadow: 0px 0px 5px 0px rgba(204,0,0,0.4);
      }
        
      .message[class*='error'] .messageTable {
        width: 100%;
      }
        
      .message[class*='error'] .messageText {
        border-bottom: 1px solid grey;
        height: 30px; 
      }
            .ques-block{
                margin-bottom: 10px;
                width: 100%;
            }
            .answ-block{
                margin-top: 10px;
            }  
            .emp-history{
                margin-bottom : 20px;
            }
            
            .parsley-errors-list {
                margin-top: 20px;
                margin-left: -50px;
                position: absolute;
                width:50%;
              }
              .desc-panel .parsley-errors-list {
                margin-top: -20px;
                margin-left: 0px;
                position: relative;
                width:100%;
              }
              .desc-panel .parsley-error {
                border: 2px solid #c00;
              }
              .emp-history .parsley-errors-list {
                margin-top: -20px;
                margin-left: 0px;
                position: relative;
                width:100%;
              }
              
              .person .parsley-errors-list {
                margin-top: 0px;
                margin-left: 0px;
                position: relative;
                width:100%;
              } 
            .md-button.md-raised.md-eec, .md-button.md-raised.md-eec {
                color: rgb(250,250,250);
                background-color: #853F3F; 
              }
              .md-button.md-raised.md-eec:hover, .md-button.md-raised.md-eec:hover {
                color: rgb(250,250,250);
                background-color: #703E3E; 
              }
              .md-button.md-raised, .md-button.md-raised {  
                background-color: #f7f7f7;
              }
              .md-button span {
                  padding: 6px;
              } 
              .top-level-label {
                font-weight: 600;
                margin-right: 5px;
                line-height: 20px; 
                color: rgba(0,0,0,0.54);
              }
              table{
                 border-collapse: separate;
                  border-spacing: 2px;
              }
              input[type="radio"], input[type="checkbox"] {
                  margin: 4px 7px 0;
             }
             .reasonErrorMessage{
                display : none;
              }
             .visible{
                display : block;
             }
             .command-button-container {
                position: relative;
                top: -2px; 
              }
               .commandBtn {
                margin: -1px 0px -2px;
              } 
             .transaction-info-row {
                margin-left: 0px; 
              }
              .required-input-with-error{
                margin-bottom : 10px;
              }
                
              .eec-subhead {
                font-size: 1.2em; 
                font-weight: 600;
                margin-left: 0px;
              }
               
      /* @see Angular Material - Input */
      .requiredInput, md-input-container span {
        -webkit-flex: 1 1 auto;
        -ms-flex: 1 1 auto;
        flex: 1 1 auto;
        -webkit-order: 2;
        -ms-flex-order: 2;
        order: 2;
        display: block;
        background: 0 0;
        padding: 2px 2px 1px;
        border-width: 0 0 1px;
        line-height: 26px;
        -ms-flex-preferred-size: 26px;
        border-radius: 0; 
      }
       
              .form-control{
                    color: rgba(0,0,0,.87);
              }  
              .navigation-buttons {
                overflow: visible; 
              }
              .not-visible{display: none;}  
              textarea{margin-bottom : 20px;}
              label {margin-bottom: 10px;font-weight: normal;}
                .requiredInput {
                  margin-left: 0px;
                  margin-top: 6px;
                }
                .desc-panel{
                  margin-left: 15px;
                  width: 90%;
                }
                .technical-info-container {
                    margin-bottom: 100px; 
                }
                textarea.form-control {
                  margin-left: 0px;
                  width: 96%;
                }
              
              @media only screen and (min-width: 0px) and (max-width: 959px) {
                .household-form-container {
                  margin: 0 !important;
                  padding: 0 !important;
                }
                
                .navigation-buttons .md-button {
                  width: 98%;
                }
                
                .navigation-buttons md-content {
                  width: 100%;
                }
                
                .add-button-container {
                  padding-right: 0;
                  width: 100%;
                }
                }
               .parsley-errors-list {
                    margin-top: 0px;
                    margin-bottom: 0px;
                    margin-left: -50px;
                    position: relative;
                    width: 50%;
                }
    </style>
        <script>

        </script>
    </head>
     <body ng-app="eecApp">
      <c:EEC_Nav_Bar id="navBar"  rendered="{!loggedUser.UserType == 'PowerPartner' || loggedUser.UserType == 'PowerCustomerSuccess' || loggedUser.UserType == 'CspLitePortal' || loggedUser.UserType == 'CustomerSuccess'}"/>
      <div class="main-content-container" layout-wrap="true" layout-lg="row" layout-gt-lg="row" layout="column" layout-padding="true" layout-margin="true" ng-controller="TransactionController as ctrl">
       <div class="background-form-container col-lg-8 col-md-8 col-sm-12" style="margin-bottom:100px;" layout="column" layout-padding="true" flex="100" flex-lg="70" flex-gt-lg="70">    
         <apex:form id="ownerShipAdminForm" styleClass="transaction-parsley-form involvment-form">
         <apex:messages id="showmsg" styleClass="error-container" style="color: #FF0000; font-weight: bold;margin-top:15px;"/>
      
        <h3 class="md-display-1">TRANSACTION - SUMMARY AND SUBMISSION PAGE - GSA</h3>
          <md-divider class="ng-cloak" layout-align="center center"></md-divider>
          <div class="transaction-info">
            <h3 class="eec-subhead">Transaction Information</h3>
            <md-content class="transaction-info-row" layout="row">
              <p class="md-subhead top-level-label">Provider Name: </p>
              <apex:outputtext value="{!licenseTransation.Provider__r.Name}" />
            </md-content>
            <md-content class="transaction-info-row" layout="row">
              <p class="md-subhead top-level-label">Transaction Type: </p>
              <apex:outputtext value="{!LicenseRecordTypeLabel}" />
            </md-content>
            <md-content class="transaction-info-row"  layout="row">
              <p class="md-subhead top-level-label">Transaction Number:</p>
              <apex:outputtext value="{!licenseTransation.Name}" />
            </md-content>
          </div>
            <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">
                Program Information</h3>
            </div>
          <div class="panel-body">
             <apex:outputpanel id="progname">
                 <apex:outputtext styleclass="top-level-label" value="Program Name:" />
                  <apex:outputtext value="{!licenseTransation.Provider__r.Name}" />
             </apex:outputpanel>
            <div>
                <apex:outputpanel id="transactionTypeTA" >
                    <apex:outputtext styleclass="top-level-label" value="Do you already hold a Dept. of Public Health Summer Camp Permit?" />
                    <apex:selectradio value="{!licenseTransation.Provider__r.Summer_Camp_Permit__c}" disabled="true">
                    <apex:selectoptions value="{!items}" />
                   
                    </apex:selectradio>
                </apex:outputpanel>
            </div>
            <div>
                <apex:outputpanel id="CampPermitIssueDate" rendered="{!((LicenseRecordTypeName == 'GSA_New_License'||LicenseRecordTypeName == 'GSA_Move'||LicenseRecordTypeName == 'GSA_Renewal'||LicenseRecordTypeName == 'GSA_License_Change'||LicenseRecordTypeName == 'GSA_Move'||LicenseRecordTypeName =='GSA_Change_of_Admin_Authority') && licenseTransation.Provider__r.Summer_Camp_Permit__c == 'Yes')}">
                    <apex:outputLabel styleclass="top-level-label" value="Camp Permit Issue Date" />
                    <apex:outputField value="{!licenseTransation.Provider__r.Camp_Permit_Issue_Date__c}"  />
                </apex:outputpanel>
            </div>
              <div>
                <apex:outputpanel id="GroupType" rendered="{!(LicenseRecordTypeName !='GSA_Change_of_Admin_Authority')}">
                    <apex:outputtext styleclass="top-level-label" value="Will your group be a small group (<=10) or a large group (>10)" />
                    <apex:selectradio value="{!licenseTransation.Group_Type__c}" disabled="true">
                    
                     <apex:selectOption itemValue="Small Group (<=10)" itemLabel="Small Group (<=10)"/>
                    <apex:selectOption itemValue="Large Group (>10)" itemLabel="Large Group (>10)"/>
                    
                    </apex:selectradio>
                </apex:outputpanel>
               </div>
             </div>
         </div>
         <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">
                Current Address</h3>
            </div>
          <div class="panel-body">
            <div>
             <apex:outputpanel id="street1">
                <apex:outputLabel styleClass="top-level-label" value="Street Address 1:" />                                    
                <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Provider__r.Provider_Address_1__c}" />
             </apex:outputpanel>
                </div>
             <div>
             <apex:outputpanel id="street2">
                <apex:outputLabel styleClass="top-level-label" value="Street Address 2:" />                                    
                <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Provider__r.Provider_Address_2__c}" />
              </apex:outputpanel>
                </div>
             <div>
             <apex:outputpanel id="citytown">
               <apex:outputLabel styleClass="top-level-label" value="City/Town:" />                                    
               <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Provider__r.Provider_City__c}" />
                </apex:outputpanel>
                </div>
             <div>
             <apex:outputpanel id="zipcode">
                <apex:outputLabel styleClass="top-level-label" value="Zip Code:" />                                    
                <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Provider__r.Provider_Zip_Code__c}" />
             </apex:outputpanel>
             </div>
             <div>
             <apex:outputpanel id="state">
                <apex:outputLabel styleClass="top-level-label" value="State:" />                                    
                <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Provider__r.State__c}" />
             </apex:outputpanel>
                </div>
           </div>
          </div>
          
         <apex:outputpanel id="newad"  rendered="{!LicenseRecordTypeName = 'GSA_Move'}">
          <div class="panel panel-default ">
           <div class="panel-heading">
              <h3 class="panel-title">
              New Address
              </h3>
           </div>
           <div class="panel-body">
             <div>
             <apex:outputpanel id="newadd1" rendered="{!(LicenseRecordTypeName !='GSA_New_License'||LicenseRecordTypeName !='GSA_Renewal' || LicenseRecordTypeName != 'GSA_License_Change' || LicenseRecordTypeName != 'GSA_Change_of_Admin_Authority' )}">
                <apex:outputLabel styleClass="top-level-label" value="New Street Address 1:" />                                    
                <apex:outputField styleclass="top-level-label" value="{!licenseTransation.New_Street_Address_1__c}" />
              </apex:outputpanel>
             </div>
             <div>
            <apex:outputpanel id="newadd2" rendered="{!(LicenseRecordTypeName !='GSA_New_License'||LicenseRecordTypeName !='GSA_Renewal'||LicenseRecordTypeName !=' GSA_License_Change'||LicenseRecordTypeName !='GSA_Change_of_Admin_Authority')}">
                <apex:outputLabel styleClass="top-level-label" value="New Street Address 2:" />                                    
                <apex:outputField styleclass="top-level-label" value="{!licenseTransation.New_Street_Address_2__c}" />
              </apex:outputpanel>
              </div>
             <div>
             <apex:outputpanel id="newcitytown" rendered="{!(LicenseRecordTypeName !='GSA_New_License'||LicenseRecordTypeName !='GSA_Renewal'||LicenseRecordTypeName !='    GSA_License_Change'||LicenseRecordTypeName !='GSA_Change_of_Admin_Authority')}">
                <apex:outputLabel styleClass="top-level-label" value="New City/Town:" />                                    
                <apex:outputField styleclass="top-level-label" value="{!licenseTransation.New_City__c}" />
              </apex:outputpanel>
                </div>
             <div>
             <apex:outputpanel id="newzipcode" rendered="{!(LicenseRecordTypeName !='GSA_New_License'||LicenseRecordTypeName !='GSA_Renewal'||LicenseRecordTypeName !=' GSA_License_Change'||LicenseRecordTypeName !='GSA_Change_of_Admin_Authority')}">
                <apex:outputLabel styleClass="top-level-label" value="New Zip Code:" />                                    
                <apex:outputField styleclass="top-level-label" value="{!licenseTransation.New_Zip_Code__c}" />
             </apex:outputpanel>
            </div>
             <div>
             <apex:outputpanel id="movingdate" >
               <apex:outputLabel styleClass="top-level-label" value="Moving Date:" />                                    
               <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Moving_Date__c}" />
             </apex:outputpanel>
            </div>
             <div>
            <apex:outputpanel id="datecare" rendered="{!(LicenseRecordTypeName !='GSA_New_License'||LicenseRecordTypeName !='GSA_Renewal'||LicenseRecordTypeName !='    GSA_License_Change'||LicenseRecordTypeName !='GSA_Change_of_Admin_Authority')}">
              <apex:outputLabel styleClass="top-level-label" value="Date Care Resumes:" />                                    
              <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Date_Care_Resumes__c}" />
            </apex:outputpanel>
          </div>
            <div>
            <apex:outputpanel id="newaddphnnum" rendered="{!(LicenseRecordTypeName !='GSA_New_License'||LicenseRecordTypeName !='GSA_Renewal'||LicenseRecordTypeName !='    GSA_License_Change'||LicenseRecordTypeName !='GSA_Change_of_Admin_Authority')}">
             <apex:outputLabel styleClass="top-level-label" value="New Address Phone Number:" />                                    
             <apex:outputField styleclass="top-level-label" value="{!licenseTransation.New_Address_Phone_Number__c}" />
            </apex:outputpanel>
            </div>
           </div>
          </div>
          </apex:outputpanel>
          <apex:outputpanel id="mmailadd" rendered="{! OR(licenseTransation.Provider__r.ShippingStreet!= null,licenseTransation.Provider__r.ShippingCity!= null,licenseTransation.Provider__r.ShippingPostalCode!= null,licenseTransation.Provider__r.ShippingState!= null)}">   
          <div class="panel panel-default ">
           <div class="panel-heading">
              <h3 class="panel-title">
             Mailing Address
              </h3>
           </div>
             <div class="panel-body">
             <div>
             <apex:outputpanel id="mailadd" rendered="{!licenseTransation.Provider__r.ShippingStreet!= null}">
                <apex:outputLabel styleClass="top-level-label" value="Mailing Address:" />                                    
                <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Provider__r.ShippingStreet}" />
                <p class="{!IF(licenseTransation.Provider__r.ShippingStreet!= null, '', 'empty-input')}"></p>
               </apex:outputpanel>
                </div>
                <div>
                <apex:outputpanel id="mailcity" rendered="{!licenseTransation.Provider__r.ShippingCity!= null}">
                    <apex:outputLabel styleClass="top-level-label" value="Mailing City/Town:" />                                    
                   <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Provider__r.ShippingCity}" />
                <p class="{!IF(licenseTransation.Provider__r.ShippingCity!= null, '', 'empty-input')}"></p>
                </apex:outputpanel>
                </div>
                <div>
             <apex:outputpanel id="mailzipcode" rendered="{!licenseTransation.Provider__r.ShippingPostalCode!= null}">
                    <apex:outputLabel styleClass="top-level-label" value="Mailing Zip Code:" />                                    
                   <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Provider__r.ShippingPostalCode}" />
                <p class="{!IF(licenseTransation.Provider__r.ShippingPostalCode!= null, '', 'empty-input')}"></p>
                </apex:outputpanel>
                </div>
                <div>
             <apex:outputpanel id="mailstate" rendered="{!licenseTransation.Provider__r.ShippingState!= null}">
                    <apex:outputLabel styleClass="top-level-label" value="Mailing State:" />                                    
                   <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Provider__r.ShippingState}" />
                <p class="{!IF(licenseTransation.Provider__r.ShippingState!= null, '', 'empty-input')}"></p>
                </apex:outputpanel>
                </div>
           </div>
          </div>
           </apex:outputpanel> 
            <div class="panel panel-default ">
            <div class="panel-heading">
              <h3 class="panel-title">
             Contact Information
              </h3>
            </div>
             <div class="panel-body">
             <div>
             <apex:outputpanel id="phnnum">
                <apex:outputLabel styleClass="top-level-label" value="Phone Number:" />                                    
               <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Provider__r.Phone}" />
                </apex:outputpanel>
                </div>
              <div>
             <apex:outputpanel id="altphnum">
                <apex:outputLabel styleClass="top-level-label" value="Alternate Phone Number:" />                                    
               <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Provider__r.Alternate_Phone__c}" />
                </apex:outputpanel>
                </div>
             <div>
             <apex:outputpanel id="fax">
                <apex:outputLabel styleClass="top-level-label" value="Fax Number:" />                                    
               <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Provider__r.Fax}" />
                </apex:outputpanel>
                </div>
             <div>
             <apex:outputpanel id="email">
                <apex:outputLabel styleClass="top-level-label" value="Email address:" />                                    
               <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Provider__r.Provider_Email__c}" />
                </apex:outputpanel>
                </div>
            </div>
            </div>
            
            <apex:outputpanel id="owner" rendered="{!LicenseRecordTypeName !='GSA_License_Change'}" >
            <div class="panel panel-default ">
            <div class="panel-heading">
              <h3 class="panel-title">
             Ownership
              </h3>
            </div>
             <div class="panel-body">
             <div>
             <apex:outputpanel id="ownership">
                <apex:outputLabel styleClass="top-level-label" value="Type of Ownership:" />                                    
              <apex:selectradio value="{!licenseTransation.Ownership_Type__c}" disabled="true">
                                        <apex:selectoptions value="{!typeOfOwnership}" >                                     
                                        </apex:selectoptions>                                        
                                      </apex:selectradio>
                </apex:outputpanel>
                </div>
                </div>
                </div>
             </apex:outputpanel>
          <apex:outputpanel id="indiowner" rendered="{!(licenseTransation.Ownership_Type__c ='Individual' || licenseTransation.Ownership_Type__c ='Doing Business As (DBA)') && (LicenseRecordTypeName !='GSA_License_Change' && ownerContactRole != null)}">
            <div class="panel panel-default ">
            <div class="panel-heading">
              <h3 class="panel-title">
             Individual Owner
              </h3>
            </div>
             <div class="panel-body">           
                <div id="no-more-tables">
                
                
                
                <table class="table  table-condensed cf">   
                  <tr>
                      <td class="top-level-label col-md-2">First Name:</td>
                   <td class="top-level-label col-md-4"><apex:outputField value="{!ownerContactRole.contact.FirstName}"/></td>
                      <td class="top-level-label col-md-2">Last Name:</td>
                       <td class="top-level-label col-md-4"><apex:outputField value="{!ownerContactRole.contact.lastname}"/></td>
                      </tr>
                      <tr>
                      <td class="top-level-label col-md-2">Street Address:</td>
                   <td class="top-level-label col-md-4"><apex:outputField value="{!ownerContactRole.contact.MailingStreet}"/></td>
                      <td class="top-level-label col-md-2">City/Town:</td>
                          <td class="top-level-label col-md-4"><apex:outputField value="{!ownerContactRole.contact.MailingCity}"/></td>
                    </tr>
                     <tr>
                      <td class="top-level-label col-md-2">Zip Code:</td>
                    <td class="top-level-label col-md-4"><apex:outputField value="{!ownerContactRole.contact.MailingPostalCode}"/></td>
                      <td class="top-level-label col-md-2">State:</td>
                        <td class="top-level-label col-md-4"><apex:outputField value="{!ownerContactRole.contact.MailingState}"/></td>
                      </tr>
                      <tr>
                      <td class="top-level-label col-md-2">Phone Number:</td>
                    <td class="top-level-label col-md-4"><apex:outputField value="{!ownerContactRole.contact.Phone}"/></td>
                      <td class="top-level-label col-md-2">Email Address:</td>
                      <td class="top-level-label col-md-4"><apex:outputField value="{!ownerContactRole.contact.Email}"/></td>
                    </tr>
                    <tr>
                       <td class="top-level-label">Does Owner hold a Family Child Care license?</td>
                      <td class="top-level-label"></td>
                      <apex:selectradio value="{!GroupType}" disabled="true">
                        <apex:selectoptions value="{!items}" />
                    </apex:selectradio>
                 </tr>
                  </table>
                 
                  </div>
                  </div>
                  </div>
                   </apex:outputpanel>
                   
             <apex:outputpanel layout="block" rendered="{!(licenseTransation.Ownership_Type__c="Partnership" && contactrolelist.size>0) && (LicenseRecordTypeName !='GSA_License_Change')}">
            <div class="panel panel-default ">
            <div class="panel-heading">
            <h3 class="panel-title">
                Partners
              </h3>
             </div> 
              
              <div layout="row" layout-wrap="true">
                <apex:repeat value="{!contactrolelist}" var="partner">
                    <div class="col-md-6 col-sm-12" style="padding: 0;">
                        <div class="slds-box slds-box--x-small " layout="column" layout-padding="true" layout-margin="true" style="overflow: hidden;">
                        <div layout="card-block row" layout-wrap="true">
                            <div class="card-row-item col-md-6 col-sm-12">
                                <p class="top-level-label">First Name</p>
                                <p class="{!IF(partner.contact.FirstName != null, '', 'empty-input')}">{!IF(partner.contact.FirstName != null, partner.contact.FirstName, partner.contact.FirstName)}</p>
                            </div>
                            <div class="card-row-item col-md-6 col-sm-12">
                                <p class="top-level-label">Last Name</p>
                                <p class="{!IF(partner.contact.LastName != null, '', 'empty-input')}">{!IF(partner.contact.LastName != null, partner.contact.LastName, partner.contact.LastName)}</p>
                            </div>
                            <div class="card-row-item col-md-6 col-sm-12">
                                <p class="top-level-label">Street Address</p>
                                <p class="{!IF(partner.contact.MailingStreet != null, '', 'empty-input')}">{!IF(partner.contact.MailingStreet != null, partner.contact.MailingStreet, partner.contact.MailingStreet)}</p>
                            </div>
                            <div class="card-row-item col-md-6 col-sm-12">
                                <p class="top-level-label">City/Town</p>
                                <p class="{!IF(partner.contact.MailingCity != null, '', 'empty-input')}">{!IF(partner.contact.MailingCity != null, partner.contact.MailingCity, partner.contact.MailingCity)}</p>
                            </div>
                            <div class="card-row-item col-md-6 col-sm-12">
                                <p class="top-level-label">Zip Code</p>
                                <p class="{!IF(partner.contact.MailingPostalCode != null, '', 'empty-input')}">{!IF(partner.contact.MailingPostalCode != null, partner.contact.MailingPostalCode, partner.contact.MailingPostalCode)}</p>
